<% content_for(:title, t('.title')) %>
<% breadcrumb :my_post %>
<h1 class="text-2xl md:text-3xl font-bold text-center p-3"><%= t('.title') %></h1>
<%= render 'shared/search', q: @q, url: posts_users_path %>
<% if @posts.present? %>
  <%= render 'shared/posts', url: posts_users_path %>
<% else %>
  <%= render 'no_posted_message' %>
<% end %>